#include<stdio.h>

float area(float l,float b)
{
                return (l*b);
}
 
 int main()
 {
 
                        float l,b,a;
 
                                        printf("Enter the length of the rectangle: ");
                                                scanf("%f",&l);
                                                printf("Enter the width of the rectangle: ");
                                                scanf("%f",&b);
                                                a=area(l,b);
                                                printf("The area of the rectangle: %.0f",a);
                                                return 0;
        }
