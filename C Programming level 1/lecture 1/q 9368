#include <stdio.h>
#include <math.h>
int main()
{
        float a,b,c,area,p;
        printf("Enter the length of three sides of triangle : ");
        scanf("%f%f%f",&a,&b,&c);
        p=(a+b+c)/2;
        area = sqrt(p*(p-a)*(p-b)*(p-c));
        printf("Area of triangle : %f\n",area);
        return(0);
}
