#include<stdio.h>

int main() {
           int arr[20], i, j, k, size;
           
              printf("Enter array size : ");
                 scanf("%d", &size);
                 
                    printf("Enter %d array elements : ",size);
                       for (i = 0; i < size; i++)
                             scanf("%d", &arr[i]);
                             
                                printf("After eliminating duplicates, the array is : ");
                                   for(i = 0; i < size; i++) {                                                   
                                        for(j = i + 1; j < size;) {
                                                         if (arr[j] == arr[i]) {
                                    for (k = j; k < size; k++) {
                                               arr[k] = arr[k + 1];
                                    }
                                                size--;
                 } else
                             j++;
}
}

   for (i = 0; i < size; i++) {
              printf("%d ", arr[i]);
   }
       printf("\n");
          return (0);
          }
